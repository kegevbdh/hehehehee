if not _G._DiqalamRan then
    _G._DiqalamRan = true

    -- Queue the script again for next teleport
    local url = "https://raw.githubusercontent.com/kegevbdh/hehehehee/main/diqalam"
    queue_on_teleport('loadstring(game:HttpGet("' .. url .. '"))()')

    -- Run the main script
    loadstring(game:HttpGet('https://raw.githubusercontent.com/ThundarZ/Welcome/refs/heads/main/Main/GaG/Main.lua'))()
end
